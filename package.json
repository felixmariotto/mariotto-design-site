{
  "name": "mariotto-design-site",
  "version": "0.0.0",
  "description": "mariottodesign.com site",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "heroku-postbuild": "npx webpack --config webpack.config.js --env NODE_ENV=prod",
    "dev": "webpack-dev-server --open --env NODE_ENV=dev",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npx webpack --config webpack.config.js --env NODE_ENV=prod",

    "split-gh-pages": "git subtree split --prefix dist -b gh-pages",
    "f-push-gh-pages": "git push -f origin gh-pages:gh-pages",
    "delete-gh-branch": "git branch -D gh-pages",

    "publish-pages": "npm run -s build && npm run -s split-gh-pages && npm run -s f-push-gh-pages && npm run -s publish-pages"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/felixmariotto/mariotto-design-site.git"
  },
  "author": "felixmariotto@gmail.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/felixmariotto/mariotto-design-site/issues"
  },
  "homepage": "https://github.com/felixmariotto/mariotto-design-site#readme",
  "dependencies": {
    "css-loader": "^6.7.0",
    "express": "^4.17.3",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "style-loader": "^3.3.1",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4"
  }
}
